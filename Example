[Python]
cisco_asa_name = 'cisco_asa'
en_password='test'

[Cisco_ASA]
enable        ...== Password                    #  "...==" means waiting. Here is waiting string 'Password' show up
$en_password  ...== $cisco_asa_name+'#'         #   waiting string 'cisco_asa#' show up
show run      ...-> $output_run                 #   "...->':  put output to a variable, which can be used in python script

[Python]
print(output_run)
